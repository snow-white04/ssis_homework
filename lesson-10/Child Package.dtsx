	{
		public void Main()
		{

            string filepath = "C:\\Users\\user\\Desktop\\SSIS\\Classes\\10\\task3_company_data.csv";
            int counter = (int)Dts.Variables["User::Counter"].Value;
            counter = 0;
            
            while (counter <= 6)
